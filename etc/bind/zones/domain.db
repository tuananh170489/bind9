;
; BIND data file for local loopback interface
;
$TTL  86400
@ IN  SOA ns1.ghtklab.com. root.ghtklab.com. (
   2011071001   ; Serial
         3600   ; Refresh
         1800   ; Retry
       604800   ; Expire
        86400 ) ; Negative Cache TTL

; name servers - NS records
  IN  NS  ns1.ghtklab.com.

; name servers - A records
  IN  A 192.168.2.101

; Hostname records
* IN  A 192.168.2.101
